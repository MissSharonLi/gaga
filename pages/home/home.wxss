page {
  background-color: #f5f5f5 !important;
  box-sizing: border-box;
  padding-bottom: calc(env(safe-area-inset-bottom) + 96rpx);
}

.t-tabs.t-tabs--top .t-tabs__scroll {
  border-bottom: none !important;
}

.home-page-header {
  background: linear-gradient() #efeff9;
  background: linear-gradient(
    to bottom,
    #eaecfb 0,
    #ededf9 20%,
    #f0f1f3 40%,
    #f2f1f6 60%,
    #f3f2f7 80%,
    #f4f4f4 100%
  );
}

.home-page-container,
.home-page-header {
  display: block;
  padding: 0 20rpx;
}

.home-page-header .t-search__input-container {
  border-radius: 4rpx !important;
  height: 64rpx !important;
  padding-left: 15rpx !important;
}
.t-search__input-box {
  background: #fff !important;
  border-color: #fff !important;
}
.home-page-header .t-search__input {
  font-size: 24rpx !important;
  color: #999 !important;
}
.home-page-header .search {
  position: relative;
}
.home-page-header .search-submit {
  position: absolute;
  right: 10rpx;
  padding: 5rpx 15rpx;
  background-color: #5a72fa;
  color: #fff;
  top: 12rpx;
  border-radius: 4rpx;
  font-size: 22rpx;
}

.home-page-header .swiper-wrap {
  margin-top: 20rpx;
}

.home-page-header .t-image__swiper {
  width: 100%;
  height: 300rpx;
  border-radius: 10rpx;
}

.home-page-container .t-tabs {
  background: #f5f5f5 !important;
}

.home-page-container .t-tabs .t-tabs-nav {
  background-color: transparent;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #333;
}

.home-page-container .t-tabs .t-tabs-scroll {
  border: none !important;
}

/* 半个字 */
.home-page-container .tab.order-nav .order-nav-item.scroll-width {
  min-width: 165rpx;
}
.home-page-container .tab .order-nav-item.active {
  color: #fa550f !important;
}

.home-page-container .tab .bottom-line {
  border-radius: 4rpx;
}

.home-page-container .tab .order-nav-item.active .bottom-line {
  background-color: #fa550f !important;
}

.home-page-container .tabs-external__item {
  /* color: #666 !important; */
  font-size: 28rpx;
}

.home-page-container .tabs--t-tabs__item-inner--active.tabs--t-tabs__item-inner--tag {
  border: 1rpx solid #5c6ef4 !important;
  font-weight: normal !important ;
  color: #5a72fa !important;
}
.home-page-container .tabs--t-tabs__item-inner--tag {
  background-color: #fff;
  border: 1rpx solid #fff;
  height: 60rpx;
}
.t-tabs__item--tag:not(.t-tabs__item--evenly):first-child {
  margin-left: 0 !important;
  padding-left: 0 !important;
}
.home-page-container .goods-list-container {
  background: #f5f5f5 !important;
  margin-top: 16rpx;
}

.home-page-tabs {
  --td-tab-nav-bg-color: transparent;
  --td-tab-border-color: transparent;
  --td-tab-item-color: #666;
  --td-tab-track-color: red;
  /* margin: -20rpx 0 -15rpx; */
}
.home-title-content {
  position: fixed;
  width: 100%;
  z-index: 999;
  top: 0;
}
.home-main-title-content {
  background-color: #ebebfd;
  padding: 10rpx;
  display: flex;
  align-items: center;
  padding-left: 25rpx;
}
.home-main-title-content .logo-content {
  position: relative;
  height: 60rpx;
  width: 230rpx;
}
.home-main-title-content .logo {
  position: absolute;
  display: block;
  width: 38rpx;
  height: 38rpx;
  z-index: 1;
  top: 11rpx;
  left: 11rpx;
}
.home-main-title-content .title {
  position: absolute;
  height: 58rpx;
  width: 180rpx;
  left: -11rpx;
  border: 2rpx solid #eee;
  border-radius: 40rpx;
  text-align: right;
  padding-right: 20rpx;
  background-color: #fff;
  line-height: 2.2;
  font-size: 28rpx;
}
.title-content {
  font-size: 30rpx;
  color: #000;
  margin-left: 40rpx;
}
.home-custom-bar {
  background: url('data:image/png;base64,')
    no-repeat;
  background-size: 100% 100%;
}
.home-custom-bar .t-navbar__content {
  width: 100%;
  background: transparent !important;
}
.t-notice-bar--warning {
  color: #e69249 !important;
  background-color: #fef6e9 !important;
  padding: 10rpx 0 !important;
  margin: 12rpx 0 0;
}
.t-notice-bar__content--vertical {
  color: #e69249;
  text-align: center;
}
page {
  background-color: #f4f4f4;
}
.submit__icon {
  position: fixed;
  right: 20rpx;
  bottom: 200rpx;
  width: 140rpx;
  height: 150rpx;
  z-index: 99;
}
