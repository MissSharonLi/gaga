page {
  background-color: #f5f5f5 !important;
}

.user-content-wrap {
  padding: 30rpx;
}
.user-avator-content {
  display: flex;
  align-items: center;
  font-size: 34rpx;
  color: #141414;
  margin-bottom: 50rpx;
  color: #333;
}
.user-avator-content .avator {
  width: 128rpx;
  height: 128rpx;
  margin-right: 40rpx;
  border-radius: 50%;
}
.user-avator-content .tel {
  color: #999;
  font-size: 28rpx;
  margin-top: 10rpx;
}
.user-douyin-content {
  background: linear-gradient(
    to right,
    #26123d 0,
    #24123b 30%,
    #1d1130 60%,
    #1b102c 90%,
    #120f20 100%
  );
  padding: 25rpx;
  display: flex;
  align-items: center;
  border-radius: 8rpx;
  justify-content: space-between;
  margin-bottom: 30rpx;
}
.user-douyin-content .left {
  display: flex;
}
.user-douyin-content .t-image {
  width: 36rpx;
  height: 42rpx;
  margin-right: 30rpx;
}
.user-douyin-content .title {
  color: #fff;
  font-size: 32rpx;
}
.user-douyin-content .limit {
  background-color: #fff;
  border-left: 8rpx solid #62bdd8;
  border-right: 8rpx solid #dd3357;
  border-radius: 8rpx;
  float: right;
  padding: 10rpx 25rpx;
  font-size: 26rpx;
  color: #333;
}
.user-exit-content .t-cell {
  padding: 20rpx !important;
  border-radius: 8rpx !important;
}
.user-exit-content .t-cell__title-text {
  font-size: 30rpx !important;
  color: #000 !important;
}
.user-exit-content .t-image {
  width: 60rpx;
  height: 60rpx;
}
