/* @import '../../../style/global.wxss'; */
page {
  width: 100vw;
  background-color: #f5f5f5 !important;
}
.goods-detail-page {
  padding-bottom: 160rpx;
}

.goods-detail-page .goods-info {
  margin: 0 auto;
  padding: 26rpx 0 28rpx 30rpx;
  background-color: #fff;
}

.goods-detail-page .swipe-img {
  width: 100%;
  height: 750rpx;
}

.goods-detail-page .goods-info .goods-price {
  display: flex;
  align-items: baseline;
}

.goods-detail-page .goods-info .goods-price-up {
  color: #fa4126;
  font-size: 28rpx;
  position: relative;
  bottom: 4rpx;
  left: 8rpx;
}

.goods-detail-page .goods-info .goods-price .class-goods-price {
  font-size: 64rpx;
  color: #fa4126;
  font-weight: bold;
  font-family: DIN Alternate;
}

.goods-detail-page .goods-info .goods-price .class-goods-symbol {
  font-size: 36rpx;
  color: #fa4126;
}

.goods-detail-page .goods-info .goods-price .class-goods-del {
  position: relative;
  font-weight: normal;
  left: 16rpx;
  bottom: 2rpx;
  color: #999999;
  font-size: 32rpx;
}

.goods-detail-page .goods-info .goods-number {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.goods-detail-page .goods-info .goods-number .sold-num {
  font-size: 24rpx;
  color: #999999;
  display: flex;
  align-items: flex-end;
  margin-right: 32rpx;
}

.goods-detail-page .goods-info .goods-activity {
  display: flex;
  margin-top: 16rpx;
  justify-content: space-between;
}

.goods-detail-page .goods-info .goods-activity .tags-container {
  display: flex;
}

.goods-detail-page .goods-info .goods-activity .tags-container .goods-activity-tag {
  background: #ffece9;
  color: #fa4126;
  font-size: 24rpx;
  margin-right: 16rpx;
  padding: 4rpx 8rpx;
  border-radius: 4rpx;
}

.goods-detail-page .goods-info .goods-activity .activity-show {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fa4126;
  font-size: 24rpx;
  padding-right: 32rpx;
}

.goods-detail-page .goods-info .goods-activity .activity-show-text {
  line-height: 42rpx;
}

.goods-detail-page .goods-info .goods-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}

.goods-detail-page .goods-info .goods-title .goods-name {
  width: 600rpx;
  font-weight: 500;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  font-size: 32rpx;
  word-break: break-all;
  color: #333333;
}

.goods-detail-page .goods-info .goods-title .goods-tag {
  width: 104rpx;
  margin-left: 26rpx;
}

.goods-detail-page .goods-info .goods-title .goods-tag .shareBtn {
  border-radius: 200rpx 0px 0px 200rpx;
  width: 100rpx;
  height: 96rpx;
  border: none;
  padding-right: 36rpx !important;
}

.goods-detail-page .goods-info .goods-title .goods-tag .shareBtn::after {
  border: none;
}

.goods-detail-page .goods-info .goods-title .goods-tag .btn-icon {
  font-size: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 96rpx;
  color: #999;
}

.goods-detail-page .goods-info .goods-title .goods-tag .btn-icon .share-text {
  padding-top: 8rpx;
  font-size: 20rpx;
  line-height: 24rpx;
}

.goods-detail-page .goods-info .goods-intro {
  font-size: 26rpx;
  color: #888;
  line-height: 36rpx;
  word-break: break-all;
  padding-right: 30rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  white-space: normal;
  overflow: hidden;
}

.spu-select {
  height: 80rpx;
  background-color: #fff;
  margin-top: 15rpx;
  display: flex;
  align-items: center;
  padding: 5rpx 20rpx;
  font-size: 26rpx;
  border-radius: 8rpx;
  margin-bottom: 20rpx;
}

.spu-select .label {
  margin-right: 30rpx;
  text-align: center;
  flex-shrink: 0;
  color: #999999;
  font-weight: normal;
}

.spu-select .content {
  display: flex;
  flex: 1;
  justify-content: space-between;
  align-items: center;
}

.spu-select .content .tintColor {
  color: #aaa;
}

.goods-detail-page .desc-content {
  margin-top: 20rpx;
  background-color: #fff;
  padding-bottom: 120rpx;
}

.goods-detail-page .desc-content__title {
  font-size: 28rpx;
  line-height: 36rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 30rpx 20rpx;
}

.goods-detail-page .desc-content__title .img {
  width: 206rpx;
  height: 10rpx;
}

.goods-detail-page .desc-content__title--text {
  font-size: 26rpx;
  margin: 0 32rpx;
  font-weight: 600;
}

.goods-detail-page .desc-content__img {
  width: 100%;
  height: auto;
}

.goods-bottom-operation {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: #fff;
  padding-bottom: env(safe-area-inset-bottom);
}

.popup-sku-header .popup-sku-header__goods-info .popup-sku__price {
  display: flex;
  align-items: baseline;
  color: #fa4126;
  margin-top: 48rpx;
}

.popup-sku-header .popup-sku-header__goods-info .popup-sku__price .popup-sku__price-num {
  font-size: 64rpx;
  color: #fa4126;
  font-weight: bold;
  font-family: DIN Alternate;
}

.popup-sku-header .popup-sku-header__goods-info .popup-sku__price .popup-sku__price-del {
  position: relative;
  font-weight: normal;
  left: 12rpx;
  bottom: 2rpx;
  color: #999999;
  font-size: 32rpx;
}

.popup-sku-header .popup-sku-header__goods-info .popup-sku__price .popup-sku__price-symbol {
  font-size: 36rpx;
  color: #fa4126;
}

.popup-sku-header .popup-sku-header__goods-info .popup-sku__price .popup-sku__price-max-num {
  font-size: 48rpx;
}

.goods-detail-page .goods-head {
  --td-swiper-radius: 0;
  background-color: #000;
}

.t-toast__content {
  z-index: 12000 !important;
}

.comments-wrap {
  margin-top: 20rpx;
  padding: 32rpx;
  background-color: #fff;
}

.comments-wrap .comments-head {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.comments-wrap .comments-head .comments-title-wrap {
  display: flex;
}

.comments-title-label,
.comments-title-count {
  color: #333333;
  font-size: 32rpx;
  font-weight: 500;
  line-height: 48rpx;
}

.comments-rate-wrap {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
}

.comments-rate-wrap .comments-good-rate {
  color: #999999;
  font-size: 26rpx;
  font-weight: 400;
  font-style: normal;
  line-height: 36rpx;
}

.comment-item-wrap .comment-item-head {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 32rpx;
}

.comment-item-wrap .comment-item-head .comment-item-avatar {
  width: 64rpx;
  height: 64rpx;
  border-radius: 64rpx;
}

.comment-item-wrap .comment-item-head .comment-head-right {
  margin-left: 24rpx;
}

.comment-head-right .comment-username {
  font-size: 26rpx;
  color: #333333;
  line-height: 36rpx;
  font-weight: 400;
}

.comment-item-wrap .comment-item-content {
  margin-top: 20rpx;
  color: #333333;
  line-height: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
}

.swiper-custom-item,
.swiper-custom-content {
  position: relative;
}
.swiper-custom-number {
  position: absolute;
  bottom: 40rpx;
  display: flex;
  z-index: 9;
  background-color: rgba(0, 0, 0, 0.5);
  color: #fff;
  padding: 10rpx 20rpx;
  font-size: 28rpx;
  border-radius: 50rpx;
  left: 30rpx;
}
.swiper-custom-content .swiper-custom-indot {
  position: absolute;
  bottom: 30rpx;
  display: flex;
  width: 320rpx;
  left: calc(50% - 320rpx);
  z-index: 9;
  background-color: rgba(0, 0, 0, 0.5);
  border-radius: 50rpx;
  width: 360rpx;
  left: calc(50% - 180rpx);
}

.custom-indot-item {
  width: 50%;
  text-align: center;
  color: #fff;
  padding: 20rpx 0;
  font-size: 28rpx;
}
.custom-indot-item.active {
  background-color: #fff;
  border-radius: 50rpx;
  color: #000;
}

.swiper-custom-controls {
  position: absolute;
  bottom: 35rpx;
  z-index: 9;
  display: flex;
  left: 40rpx;
  width: calc(100% - 80rpx);
  justify-content: space-between;
  right: 40rpx;
}

.custom-control-content {
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.custom-controls-item {
  width: 60rpx;
  height: 60rpx;
}
.zoom {
  width: 60rpx;
  height: 60rpx;
  margin-left: 20rpx;
}
.notice-avator-img .t-image {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}
.swiper-custom__content--vertical {
  padding: 10rpx 0 10rpx 10rpx;
  border-radius: 8rpx;
  min-width: 400rpx;
  height: 50rpx;
  position: absolute;
  top: 40rpx;
  left: 20rpx;
  z-index: 99;
  color: #fff;
  font-size: 24rpx;
  line-height: 50rpx;
  background-color: rgba(0, 0, 0, 0.6);
}
.goods__info__content {
  background-color: #f5f5f5;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  padding: 20rpx;
}
.goods__info__content .info__detail__content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 10rpx 30rpx;
}
.goods__info__content .info__detail__content .info__detail__left {
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction: column;
  width: 55%;
}
.info__detail__left .left__title,
.info__detail__left .left__content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}
.info__detail__left .left__title .one,
.info__detail__left .left__content .one {
  width: 70%;
  color: red;
  font-size: 24rpx;
  display: flex;
  align-items: center;
}
.info__detail__left .left__title .two {
  color: #333;
  font-size: 24rpx;
}
.info__detail__left .left__content {
  margin-top: 20rpx;
}
.small,
.mini {
  width: 50%;
}
.mini {
  width: 80%;
  font-size: 22rpx;
}
.small,
.mini,
.one,
.two {
  display: flex;
  align-items: center;
}

.info__detail__left .left__content .one .point {
  color: red;
  font-size: 40rpx;
}
.info__detail__left .left__content .two {
  align-items: baseline;
  font-size: 24rpx;
}
.info__detail__left .left__content .price {
  font-size: 30rpx;
  padding-left: 10rpx;
}
.add__to__window {
  border-radius: 8rpx;
  padding: 10rpx 20rpx;
  font-size: 26rpx;
  border: 2rpx solid #333;
}
.product__info__content {
  background-color: #fff;
  padding: 15rpx 20rpx;
  border-radius: 8rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.product__info__content .product__detail__content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}
.product__detail__content .info__right {
  width: calc(100% - 160rpx);
  position: relative;
  height: 135rpx;
  margin-top: 5rpx;
}
.product__info__content .img .t-image {
  width: 150rpx;
  height: 150rpx;
  margin-right: 5rpx;
}
.product__info__content .info__right .douyin,
.product__info__content .info__right .douyin .t-image {
  width: 70rpx;
  height: 35rpx;
  margin-right: 15rpx;
}
.product__info__content .info__right .ellipsis {
  max-width: calc(100% - 80rpx);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #000;
  font-weight: 600;
  font-size: 30rpx;
}
.product__info__content .tips__content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  color: #333;
  position: absolute;
  bottom: 15rpx;
  left: 0;
  width: 100%;
}
.product__info__content .number__content,
.product__info__content .title {
  display: flex;
  align-items: center;
}
.product__info__content .number {
  color: #999;
  margin-right: 10rpx;
}
.goods-head .t-borderless {
  margin-top: 20rpx;
}

.goods__show__content {
  min-height: 800rpx;
}
.steps__tips__content {
  background-color: #f5f5f5;
  padding: 15rpx;
  line-height: 1.8;
  text-align: justify;
  color: #999;
  font-size: 22rpx;
  border-radius: 8rpx;
  margin-top: 30rpx;
}
.steps__draw__content {
  display: flex;
  justify-content: center;
  align-items: top;
  margin-top: 30rpx;
}
.steps__draw__content .step {
  display: flex;
  flex-direction: column;
  text-align: center;
  width: 30%;
}
.steps__draw__content .step .number {
  width: 54rpx;
  height: 54rpx;
  line-height: 54rpx;
  border-radius: 50%;
  background-color: rgb(224, 229, 255);
  color: rgb(95, 108, 245);
  margin: 0 auto;
  font-weight: 800;
  font-size: 36rpx;
  position: relative;
}
.steps__draw__content .step:not(:last-child) .number:after {
  content: '......';
  position: absolute;
  left: 180%;
  top: -10rpx;
  letter-spacing: 5rpx;
  font-weight: 700;
  color: rgb(224, 229, 255);
}
.steps__draw__content .step .text {
  font-size: 22rpx;
  margin-top: 20rpx;
  color: #333;
}
.steps__video__content {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  position: relative;
}
.steps__video__content .steps__video__item {
  width: 31%;
  height: 250rpx;
  margin-bottom: 30rpx;
  border-radius: 4rpx;
  overflow: hidden;
  position: relative;
}
.steps__video__content .steps__video__item:not(:nth-child(3n)) {
  margin-right: 3.5%;
}

.steps__video__content .video__play__content {
  position: absolute;
  bottom: 15rpx;
  left: 10rpx;
  right: 15rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: calc(100% - 25rpx);
  font-size: 24rpx;
  color: #fff;
  z-index: 999;
}
.steps__video__content .video__play__content .img {
  width: 32rpx;
  height: 32rpx;
}
.steps__video__content .images__selected {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 15rpx;
  top: 15rpx;
  z-index: 999;
  border-radius: 50%;
}
/* 操作按钮 */
.goods__operation__content {
  background-color: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  width: calc(100% - 40rpx);
  height: 80rpx;
  padding: 20rpx 20rpx 50rpx;
  display: flex;
  justify-content: space-between;
  z-index: 999;
}
.goods__operation__content .help {
  width: 13%;
  font-size: 22rpx;
  text-align: center;
}
.goods__operation__content .help .t-image {
  width: 36rpx;
  height: 36rpx;
  display: block;
  margin: 0 auto 10rpx;
}
.goods__operation__content .button {
  width: 40%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 4rpx;
}
.goods__operation__content .guan .t-image {
  width: 50rpx;
  height: 50rpx;
  margin-right: 10rpx;
}
.goods__operation__content .guan {
  color: rgb(111, 125, 229);
  font-size: 24rpx;
  background-color: rgb(238, 239, 254);
}
.goods__operation__content .download__button {
  width: 40%;
  height: 80rpx;
  color: #fff;
  background-color: rgb(96, 110, 250);
  text-align: center;
  flex-direction: column;
  font-size: 24rpx;
}
.goods__operation__content .button__small {
  font-size: 20rpx;
}
.custom-toast-content .progress {
  font-size: 32rpx;
  margin: 20rpx 0 10rpx;
  text-align: center;
}
.t-toast .tips {
  font-size: 24rpx;
  text-align: center;
}
.custom-toast-content {
  background-color: #000 !important;
}
.video__swiper__item__text {
  width: 500rpx;
  margin: 0 20rpx;
  height: 400rpx;
  background-color: rgba(238, 239, 253);
  font-size: 24rpx;
  padding: 20rpx;
}

.swiperClass {
  margin: 0;
  margin-top: 10px;
  height: 300rpx;
}
.swiper__title__content {
  font-size: 24rpx;
  margin: 20rpx 0;
  padding-left: 20rpx;
  font-weight: 600;
}
.swiperClass .copy {
  position: absolute;
  bottom: 10rpx;
  left: 20rpx;
  right: 20rpx;
  font-size: 22rpx;
  color: #999;
  border-top: 1px solid #ccc;
  line-height: 50rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
}
.slide-image {
  height: 250rpx;
  border-radius: 10px;
  position: relative;
  background-color: rgba(238, 239, 253);
  font-size: 22rpx;
  padding: 20rpx;
  text-align: justify;
  position: relative;
}

.slide-image.active {
  transform: none;
  transition: all 0.1s ease-in 0s;
}

.slide-image.quiet {
  transform: scale(0.9) !important;
  transition: all 0.1s ease-in 0s;
}
.slide-image-content {
  height: 200rpx;
  overflow-y: scroll;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch;
}
