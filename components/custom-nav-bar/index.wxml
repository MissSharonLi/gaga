<t-navbar title="{{title}}" class="home-custom-bar">
  <view slot="capsule" class="{{type === 1 ? 'custom-capsule' : 'custom-transparent'}}">
    <t-icon
      size="20"
      bind:tap="onBack"
      aria-role="button"
      aria-label="返回"
      name="chevron-left"
      class="custom-capsule__icon"
    />
    <t-icon
      size="20"
      bind:tap="onGoHome"
      aria-role="button"
      aria-label="首页"
      name="home"
      class="custom-capsule__icon"
    />
  </view>
</t-navbar>

